{
  "name": "@jetpack/api",
  "version": "0.3.5",
  "description": "NestJS backend",
  "author": "Higor Alves <higorhaalves@gmail.com>",
  "private": true,
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "prestart": "rimraf dist",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "prettier": "npx prettier --write src/**/*.{ts,tsx,json}",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest -o --silent",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "clean": "rimraf dist *.tsbuildinfo"
  },
  "dependencies": {
    "@nestjs/common": "^7.6.1",
    "@nestjs/config": "^0.6.1",
    "@nestjs/core": "^7.6.1",
    "@nestjs/jwt": "^7.2.0",
    "@nestjs/mongoose": "^7.2.0",
    "@nestjs/passport": "^7.1.5",
    "@nestjs/platform-express": "^7.6.1",
    "@nestjs/swagger": "^4.7.5",
    "@sendgrid/mail": "^7.4.0",
    "@sentry/node": "^5.29.0",
    "@types/mongodb": "^3.6.3",
    "bcrypt": "^5.0.0",
    "class-validator": "^0.12.2",
    "coinbase-commerce-node": "^1.0.4",
    "express-rate-limit": "^5.2.3",
    "hbs": "^4.1.1",
    "helmet": "^4.2.0",
    "mongodb": "^3.6.3",
    "mongoose": "^5.11.7",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^6.6.3",
    "stripe": "^8.128.0",
    "swagger-ui-express": "^4.1.5",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@nestjs/cli": "^7.5.4",
    "@nestjs/schematics": "^7.2.5",
    "@nestjs/testing": "^7.6.1",
    "@types/bcrypt": "^3.0.0",
    "@types/coinbase-commerce-node": "^1.0.5",
    "@types/express": "^4.17.9",
    "@types/express-rate-limit": "^5.1.0",
    "@types/jest": "^26.0.19",
    "@types/mongoose": "^5.10.2",
    "@types/node": "^14.14.12",
    "@types/passport-jwt": "^3.0.3",
    "@types/passport-local": "^1.0.33",
    "@types/stripe": "^8.0.417",
    "@types/supertest": "^2.0.10",
    "jest": "^26.6.3",
    "supertest": "^6.0.1",
    "ts-jest": "^26.4.4",
    "ts-loader": "^8.0.12",
    "ts-node": "^9.1.1",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.1.3"
  }
}
