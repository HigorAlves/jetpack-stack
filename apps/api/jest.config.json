{
	"bail": 1,
	"moduleFileExtensions": [
		"js",
		"json",
		"ts"
	],
	"silent": true,
	"testRegex": ".*\\.spec\\.ts$",
	"transform": {
		"^.+\\.(t|j)s$": "ts-jest"
	},
	"collectCoverageFrom": [
		"**/*.(t|j)s"
	],
	"coverageDirectory": "/coverage",
	"testEnvironment": "node",
	"moduleNameMapper": {
		"~/(.*)": [
			"<rootDir>/src/$1"
		]
  }
}
